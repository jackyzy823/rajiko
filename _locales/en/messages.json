{
    "manifest_app_description": {
        "message": "Rajiko is a tool for unblocking geolocation restriction of radiko.jp! Enjoy radio of Tokyo，Aomori,and etc From Osaka,Nagazaki even overseas!"
    }
    ,"confirm_button":{
        "message":"Confirm"
    }
    ,"refresh_alert":{
        "message":"Please refresh pages of radiko.jp!"
    }
    ,"popup_title":{
        "message":"Choose Area - Rajiko"
    }
    ,"record_button_to_start":{
        "message":"Record $RADIONAME$"
        ,"placeholders":{
            "radioname":{
                "content":"$1",
                "example" : "bayfm78"
            }
        }
    }
    ,"record_button_to_stop":{
        "message":"Stop recording"
    }
    ,"record_prepare":{
        "message":"No radio playing currently! Ready to record $RADIO$ in the current page."
        ,"placeholders":{
            "radio":{
                "content" :"$1",
                "example" : "bayfm78"
            }
        }
    },"timeshift_button":{
        "message":"Download タイムシフト"
    }

}